							</view>
						</view>
						<view wx:if="{{item.is_vip}}" class="action">
							<view class="text-grey text-xs">bot</view>
							<view class="{{item.boot_open?'cuIcon-noticefill text-green':'cuIcon-notice_forbid_fill text-gray'}}">
							</view>
						</view>
					</view>
				</block>
