	<block wx:if="{{user.wxid}}">
		<!-- 加载 -->
		<view class="page-null bg-bg-white" wx:if="{{!groups_is_done}}">
			<wux-spin nested spinning="{{ !groups_is_done }}" tip="">
				<view style='height:500rpx'></view>
			</wux-spin>
		</view>

		<view hidden="{{!groups_is_done}}" class="">
