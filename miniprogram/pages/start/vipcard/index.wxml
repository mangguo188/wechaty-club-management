								<!-- <view class="cu-tag round bg-orange sm">6人</view> -->
							</view>
							<!-- <view class="cu-tag sm line-blue light round">{{item.roomid}}
							</view> -->

							<view wx:if="{{!item.announce}}" class="text-gray text-sm flex">
								{{item.roomid}}/
								<text class="cuIcon-weixin text-green margin-right-xs"></text>
								<text class="text-cut text-blue">{{item.owner_nick||item.ownerid||'待认领'}}</text>
							</view>
							<view wx:if="{{item.announce}}" class="text-gray text-sm flex">
