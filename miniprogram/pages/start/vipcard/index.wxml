									<!-- <text class="cuIcon-weixin text-green margin-right-xs"></text> -->
									{{item.announce}}
								</view>
							</view>
						</view>
						<view wx:if="{{item.is_vip}}" class="action">
							<view class="text-grey text-xs">VIP</view>
							<view class="cuIcon-notice_forbid_fill text-gray"></view>
						</view>
					</view>
				</block>

