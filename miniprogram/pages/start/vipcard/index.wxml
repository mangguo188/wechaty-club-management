				<block wx:if="{{(owner_rooms.length!=0||user.wxid)&&false}}">
					<!-- 我的群组标签 -->
					<view class="cu-list menu">
						<view class="cu-item {{menuArrow?'arrow':''}}">
							<view class="content">
								<text class="text-pink"></text>
								<text class="text-grey">我的群组 ({{owner_rooms.length}})</text>
							</view>
						</view>
					</view>
					<!-- 我的群组为空 -->
					<view wx:if="{{owner_rooms.length==0}}" class="margin-top">
						<wux-prompt visible="{{ true }}" icon="{{ msg2.icon }}" title="没有加入任何群~" text="{{ msg2.text }}"
							buttons="{{ msg2.buttons }}" bind:click="" />
					</view>
					<!-- 我的群组列表 -->
					<view class="cu-item cur" wx:for="{{owner_rooms}}" wx:key="index" bindtap="to_room_details"
						data-index="{{index}}" data-id="{{item._id}}">
						<view wx:if="{{!item.avatar||!item.avatar.remoteUrl}}" class="cu-avatar bg-blue lg radius">
							{{item.topic[0]||'匿'}}
							<view wx:if="{{item.is_vip}}" class="cu-tag badge">VIP</view>
						</view>
						<view wx:else class="cu-avatar radius lg" style="background-image:url({{item.avatar.remoteUrl}});">
							<view wx:if="{{item.is_vip}}" class="cu-tag badge">VIP</view>
						</view>
						<view class="content">
							<view>
								<view class="text-cut">
									{{item.topic||item._id}}
								</view>
								<!-- <view class="cu-tag round bg-orange sm">6人</view> -->
							</view>
							<!-- <view class="cu-tag sm line-blue light round"><text
								class="cuIcon-weixin text-blue margin-right-xs"></text>{{item.owner_nick||item.ownerid||'待认领'}}
						</view> -->
							<view wx:if="{{!item.announce}}" class="text-gray text-sm flex">
								<text class="cuIcon-weixin text-green margin-right-xs"></text>
								<text class="text-cut text-blue"> {{item.owner_nick||item.ownerid||'待认领'}}</text>
							</view>
							<view wx:if="{{item.announce}}" class="text-gray text-sm flex">
